#!/usr/bin/python
#encoding=utf-8

import sys
import os
from threading import Thread
from Ddata import trace

def main():
    f=open("targetip.txt", "r")
    lines=f.readlines()
    count=0
    for line in lines:
    	count=count+1
    for i in range(0,count,50):                
    	threads=[]
        for j in range(1):
            pos=i+j
            if pos>=count:
               break
            tgtip=lines[i+j].split('\n')[0]
            q=trace(tgtip)
	    print tgtip
            thread=Thread(target=q)
            threads.append(thread)
        length=len(threads)    
        for a in range(length):
            threads[a].start()
       	for a in range(length):
            threads[a].join()
                
        
if __name__=='__main__':
    main()
         
        
